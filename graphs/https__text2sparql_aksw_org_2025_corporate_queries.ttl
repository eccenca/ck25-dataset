SELECT ?name (ROUND(AVG(?amt) * 100) / 100 AS ?avgEur) WHERE {
  ?sup a pv:Supplier ;
    pv:name ?name .
  ?hw pv:hasSupplier ?sup ;
    pv:price/pv:amount ?amt ;
    pv:price/pv:currency \"EUR\" .
}
GROUP BY ?sup ?name
""";
