    pv:Hardware, pv:Service, pv:Supplier, pv:BomPart;
  <https://text2sparql.aksw.org/2025/corporate/queries/relatedProperty> pv:hasBomPart,
    pv:hasSupplier, pv:eligibleFor, pv:addressCountry, pv:hasPart .

<https://text2sparql.aksw.org/2025/corporate/queries/29> a shui:SparqlQuery;
  rdfs:label "What suppliers - I need name, country and price - deliver the 6th to 10th most expensive hardware items.";
  shui:queryType "SELECT";
  shui:queryText """PREFIX prodi: <http://ld.company.org/prod-instances/>
PREFIX pv: <http://ld.company.org/prod-vocab/>

SELECT ?name ?country ?price 
WHERE {
  ?sup a pv:Supplier ;
    pv:addressCountry ?country ;
  pv:name ?name .
  ?hw a pv:Hardware ;
  pv:price/pv:amount ?price ;
    pv:price/pv:currency \"EUR\" ;
    pv:hasSupplier ?sup .
}
ORDER BY DESC(?price)
LIMIT 5 OFFSET 10
""";
