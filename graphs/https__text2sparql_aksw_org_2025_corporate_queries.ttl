<https://text2sparql.aksw.org/2025/corporate/queries/37> a shui:SparqlQuery;
  rdfs:label "For each Bill of Material, how many parts does it contain and what is the total material quantity — show me only those BOMs exceeding 600 total items and ordern them descending.";
  shui:queryType "SELECT";
  shui:queryText """PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX prodi: <http://ld.company.org/prod-instances/>
PREFIX pv: <http://ld.company.org/prod-vocab/>

SELECT ?bom (COUNT(?part) AS ?partCount) (SUM(?qty) AS ?totalQty)
WHERE {
  ?bom a pv:BillOfMaterial ;
    pv:hasBomPart ?part .
  ?part pv:quantity ?qty_ .
  BIND(xsd:int(?qty_) AS ?qty)
}
GROUP BY ?bom
HAVING (SUM(?qty) > 600)
ORDER BY DESC(SUM(?qty))
""";
  dcterms:created "2025-04-29T17:36:57.904Z"^^xsd:dateTime;
  dcterms:modified "2025-04-29T17:36:57.904Z"^^xsd:dateTime;
