<https://text2sparql.aksw.org/2025/corporate/queries/41> a shui:SparqlQuery;
  rdfs:label "For each manager, what percentage of their entire team work in the same department as the manager?";
  shui:queryType "SELECT";
  shui:queryText """PREFIX prodi: <http://ld.company.org/prod-instances/>
PREFIX pv: <http://ld.company.org/prod-vocab/>

SELECT ?m ?name (?deptTeam / ?fullteam * 100 AS ?pct)
WHERE {
  
  ?m pv:name ?name .
  
  { # full team
    SELECT ?m (COUNT(*) AS ?fullteam) WHERE {
      ?e pv:hasManager ?m .
    } GROUP BY ?m
  }
        
  { # team in same dept
    SELECT ?m (COUNT(*) AS ?deptTeam) WHERE {
      ?m pv:memberOf ?dept .
      ?e pv:hasManager ?m .
      ?e pv:memberOf ?dept .
    } GROUP BY ?m
  }
}
""";
  dcterms:created "2025-04-29T17:36:57.906Z"^^xsd:dateTime;
  dcterms:modified "2025-04-29T17:36:57.906Z"^^xsd:dateTime;
