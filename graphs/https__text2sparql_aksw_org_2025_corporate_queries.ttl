<https://text2sparql.aksw.org/2025/corporate/queries/38> a shui:SparqlQuery;
  rdfs:label "I want to update my contact list, for each Employee give me name, email, phone number and the department they belong to as well as their direct report.";
  shui:queryType "SELECT";
  shui:queryText """PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX prodi: <http://ld.company.org/prod-instances/>
PREFIX pv: <http://ld.company.org/prod-vocab/>

SELECT ?name ?email ?phone ?dept ?manager
WHERE {
  ?agentClass rdfs:subClassOf* pv:Agent .
  ?empl a ?agentClass ;
    pv:name ?name .
  OPTIONAL { ?empl pv:email ?email . }
  OPTIONAL { ?empl pv:phone ?phone . }
  OPTIONAL { ?empl pv:memberOf/pv:name ?dept . }
  OPTIONAL { ?empl pv:hasManager/pv:name ?manager . }
}
""";
  dcterms:created "2025-04-29T17:36:57.905Z"^^xsd:dateTime;
  dcterms:modified "2025-04-29T17:36:57.905Z"^^xsd:dateTime;
