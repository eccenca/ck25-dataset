    pv:Hardware, pv:Price, pv:BomPart;
  <https://text2sparql.aksw.org/2025/corporate/queries/relatedProperty> pv:hasBomPart,
    pv:price, pv:amount, pv:currency, pv:hasPart, pv:quantity .

<https://text2sparql.aksw.org/2025/corporate/queries/43> a shui:SparqlQuery;
  rdfs:label "Show me any cycles of product compatibility — i.e. product A says it's compatible with B, and B says it's compatible with A (mutual pairs).";
  shui:queryType "SELECT";
  shui:queryText """PREFIX pv: <http://ld.company.org/prod-vocab/>

SELECT ?a ?b
WHERE {
  ?a pv:compatibleProduct ?b .
  FILTER (STR(?a) < STR(?b))
}
""";
  dcterms:created "2025-04-29T17:36:57.909Z"^^xsd:dateTime;
