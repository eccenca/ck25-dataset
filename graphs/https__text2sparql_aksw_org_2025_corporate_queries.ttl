<https://text2sparql.aksw.org/2025/corporate/queries/15> a shui:SparqlQuery;
  rdfs:label "What is the cheapest Encoder we can get from a french or german supplier?";
  shui:queryType "SELECT";
  shui:queryText """PREFIX pv: <http://ld.company.org/prod-vocab/>
SELECT DISTINCT ?result
WHERE
{
  ?result pv:hasCategory <http://ld.company.org/prod-instances/prod-cat-Encoder> .
  ?result pv:price ?priceR .
  ?priceR pv:amount ?price .
  ?result pv:hasSupplier ?supplier .
  ?supplier pv:addressCountry ?country .
  FILTER ( (?country = \"France\") ||  (?country = \"Germany\") )
}
ORDER BY ASC(?price)
LIMIT 1
""";
  dcterms:created "2025-04-29T17:36:57.889Z"^^xsd:dateTime;
  dcterms:modified "2025-04-29T17:36:57.889Z"^^xsd:dateTime;
