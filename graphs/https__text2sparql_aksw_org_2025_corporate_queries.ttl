<https://text2sparql.aksw.org/2025/corporate/queries/28> a shui:SparqlQuery;
  rdfs:label "Do we have any service that does apply to a BOM where parts are sourced from Russia?";
  shui:queryType "SELECT";
  shui:queryText """PREFIX prodi: <http://ld.company.org/prod-instances/>
PREFIX pv: <http://ld.company.org/prod-vocab/>

ASK 
WHERE {
  { # all BOMs with parts from russia
    SELECT ?bom
    WHERE {
      ?bom pv:hasBomPart ?bomPart .
      ?bomPart pv:hasPart ?hw .
      ?hw pv:hasSupplier ?suppl .
      ?suppl pv:addressCountry ?country .
      FILTER (CONTAINS(LCASE(?country), LCASE(\"Russia\")))
    }
  }
  
  # services eligible for parts associated with these BOMs?
  ?s a pv:Service .
  ?s pv:eligibleFor ?hw .
  ?bom pv:hasBomPart ?bomPart .
  ?bomPart pv:hasPart ?hw .
}
""";
  dcterms:created "2025-04-29T17:36:57.896Z"^^xsd:dateTime;
  dcterms:modified "2025-04-29T17:36:57.896Z"^^xsd:dateTime;
