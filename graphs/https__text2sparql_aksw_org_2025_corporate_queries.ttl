    pv:Department;
  <https://text2sparql.aksw.org/2025/corporate/queries/relatedProperty> pv:name, pv:hasManager,
    pv:memberOf .

<https://text2sparql.aksw.org/2025/corporate/queries/42> a shui:SparqlQuery;
  rdfs:label "Which Bill-of-Material has the highest average unit cost of its hardware parts, and what is that average?";
  shui:queryType "SELECT";
  shui:queryText """PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX prodi: <http://ld.company.org/prod-instances/>
PREFIX pv: <http://ld.company.org/prod-vocab/>

SELECT ?bom (ROUND(AVG(?spst / ?sq) * 100) / 100 AS ?avgUnitCost)
WHERE {
  {
    SELECT ?bom (SUM(?partSubTotal) AS ?spst) (SUM(xsd:int(?quant)) AS ?sq)
    WHERE {
      ?bom a pv:BillOfMaterial ;
        pv:hasBomPart ?part . 
      ?part pv:quantity ?quant .
      ?part pv:hasPart/pv:price/pv:amount ?amount .
      ?part pv:hasPart/pv:price/pv:currency \"EUR\" .
      BIND(xsd:int(?quant) * ?amount AS ?partSubTotal)
    }
    GROUP BY ?bom
  }
} 
GROUP BY ?bom
ORDER BY DESC(?avgUnitCost)
LIMIT 1
""";
  dcterms:created "2025-04-29T17:36:57.907Z"^^xsd:dateTime;
  dcterms:modified "2025-04-29T17:36:57.907Z"^^xsd:dateTime;
