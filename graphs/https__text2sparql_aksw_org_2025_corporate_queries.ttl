    <https://text2sparql.aksw.org/2025/corporate/queries/features/FILTER>, <https://text2sparql.aksw.org/2025/corporate/queries/features/LIMIT>,
    <https://text2sparql.aksw.org/2025/corporate/queries/features/ORDER>;
  <https://text2sparql.aksw.org/2025/corporate/queries/relatedClass> pv:Supplier, pv:Price,
    pv:ProductCategory, pv:Product;
  <https://text2sparql.aksw.org/2025/corporate/queries/relatedProperty> pv:hasCategory,
    pv:hasSupplier, pv:price, pv:addressCountry, pv:amount .

