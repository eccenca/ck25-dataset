<https://text2sparql.aksw.org/2025/corporate/queries/46> a shui:SparqlQuery;
  rdfs:label "What are the top 5 suppliers with best average reliability over their products.";
  ns2:modified "2026-01-31T07:52:01.503Z"^^xsd:dateTime;
  ns2:created "2026-01-31T07:52:01.503Z"^^xsd:dateTime;
  shui:queryText """PREFIX pv: <http://ld.company.org/prod-vocab/>
SELECT DISTINCT ?result (AVG(?reliabilityIndex) AS ?averageReliabilityIndex)
WHERE
{
  ?hardware pv:reliabilityIndex ?reliabilityIndex .
  ?hardware pv:hasSupplier ?result .
}
GROUP BY ?result
ORDER BY DESC(?averageReliabilityIndex)
LIMIT 5
""";
  shui:queryType "SELECT";
