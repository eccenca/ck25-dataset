<https://text2sparql.aksw.org/2025/corporate/queries/28> a shui:SparqlQuery;
  rdfs:label "Do we have any service that does apply to a BOM where parts are sourced from Russia?";
  ns2:modified "2026-01-31T07:52:01.496Z"^^xsd:dateTime;
  ns2:created "2026-01-31T07:52:01.496Z"^^xsd:dateTime;
  shui:queryText """PREFIX prodi: <http://ld.company.org/prod-instances/>
PREFIX pv: <http://ld.company.org/prod-vocab/>

ASK
WHERE {
  { # all BOMs with parts from russia
    SELECT ?bom
    WHERE {
      ?bom pv:hasBomPart ?bomPart .
      ?bomPart pv:hasPart ?hw .
      ?hw pv:hasSupplier ?suppl .
      ?suppl pv:addressCountry ?country .
      FILTER (CONTAINS(LCASE(?country), LCASE(\"Russia\")))
    }
  }

  # services eligible for parts associated with these BOMs?
  ?s a pv:Service .
  ?s pv:eligibleFor ?hw .
  ?bom pv:hasBomPart ?bomPart .
  ?bomPart pv:hasPart ?hw .
}
""";
  shui:queryType "SELECT";
  <https://text2sparql.aksw.org/2025/corporate/queries/feature> <https://text2sparql.aksw.org/2025/corporate/queries/features/FILTER>,
    <https://text2sparql.aksw.org/2025/corporate/queries/features/ASK>, <https://text2sparql.aksw.org/2025/corporate/queries/features/SUBSELECT>;
  <https://text2sparql.aksw.org/2025/corporate/queries/relatedProperty> pv:addressCountry,
    pv:eligibleFor, pv:hasBomPart, pv:hasPart, pv:hasSupplier;
  <https://text2sparql.aksw.org/2025/corporate/queries/relatedClass> pv:Supplier, pv:Hardware,
