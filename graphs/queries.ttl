<https://text2sparql.aksw.org/2025/corporate/queries/44> a shui:SparqlQuery;
  rdfs:label "Which hardware items have dimensions in the top 10 % of all widths, and what are their IDs, names and widths?";
  ns2:modified "2026-01-31T07:52:01.502Z"^^xsd:dateTime;
  ns2:created "2026-01-31T07:52:01.502Z"^^xsd:dateTime;
  shui:queryText """PREFIX pv: <http://ld.company.org/prod-vocab/>

SELECT ?id ?name ?width
WHERE {
  { # get min-max range
    SELECT (MIN(?width) AS ?minW) (MAX(?width) AS ?maxW)
    WHERE {
      ?hw a pv:Hardware ;
        pv:width_mm ?width .
    }
  }

  # get 90. percentile
  BIND(?minW + (0.9 * (?maxW - ?minW)) AS ?pct90)

  ?hw a pv:Hardware ;
    pv:id ?id ;
    pv:name ?name ;
    pv:width_mm ?width .

  FILTER(?width >= ?pct90)
}
ORDER BY DESC(?width)
""";
  shui:queryType "SELECT";
  <https://text2sparql.aksw.org/2025/corporate/queries/feature> <https://text2sparql.aksw.org/2025/corporate/queries/features/SELECT>,
    <https://text2sparql.aksw.org/2025/corporate/queries/features/FILTER>, <https://text2sparql.aksw.org/2025/corporate/queries/features/BIND>,
    <https://text2sparql.aksw.org/2025/corporate/queries/features/SUBSELECT>, <https://text2sparql.aksw.org/2025/corporate/queries/features/MIN>,
    <https://text2sparql.aksw.org/2025/corporate/queries/features/MAX>;
  <https://text2sparql.aksw.org/2025/corporate/queries/relatedProperty> pv:compatibleProduct,
    pv:id, pv:name, pv:width_mm;
  <https://text2sparql.aksw.org/2025/corporate/queries/relatedClass> pv:Hardware .

<https://text2sparql.aksw.org/2025/corporate/queries/45> a shui:SparqlQuery;
