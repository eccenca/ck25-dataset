<https://text2sparql.aksw.org/2025/corporate/queries/4> a shui:SparqlQuery;
  rdfs:label "What is the email of Sabrina from Marketing?";
  ns2:modified "2026-01-31T07:52:01.484Z"^^xsd:dateTime;
  ns2:created "2026-01-31T07:52:01.484Z"^^xsd:dateTime;
  shui:queryText """PREFIX pv: <http://ld.company.org/prod-vocab/>
SELECT DISTINCT ?result
WHERE
{
  ?department pv:name \"Marketing\" .
  ?person pv:memberOf ?department .
  ?person pv:email ?result .
  ?person pv:name ?name .
  FILTER (CONTAINS(?name, \"Sabrina\"))
}
""";
